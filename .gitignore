# Crear .gitignore
cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
.venv/
venv/
*.egg-info/

# Java
bin/
*.class

# Logs
logs/
*.log

# Base de datos
*.db
db/

# IDE
.idea/
.vscode/
*.swp

# OS
.DS_Store
Thumbs.db
EOF

# Binarios compilados (no deben estar en el repo)
*.class
bin/

# Cache de Python (no deben estar en el repo)
__pycache__/
*.pyc

# Logs
logs/
*.log

# Bases de datos locales (excepto reniec.db de prueba)
db/
